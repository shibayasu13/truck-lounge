<%= form_for @lounge, url:guest_lounge_path do |f| %>
	
			
				<%= f.text_field :store_name %>
			
				<%= f.text_field :prefecture %>
					<%= f.text_field :address_city %>
					<%= f.text_field :address_street %>
				
			
				<%= f.select :content, Lounge.contents.keys %>
			
				<%= f.attachment_field :store_image %>
			<%= f.text_area :parking %>
			<%= f.text_field :business_hours %>
			<%= f.select :busines_status, Lounge.busines_statuses.keys %>
	<%= f.submit '編集する' %>
<% end %>